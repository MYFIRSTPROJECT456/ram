<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<title>StateList</title>
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link rel="stylesheet" href="/css/bootstrap.css">
</head>
<body>
<%- include menu.ejs %>
<div class="container">
	<a href="/admin/state/add" style="margin-top: 5px;" type="button" class="btn btn-success btn-lg btn-block" id="btn">ADD NEW STATE</a>
	<br>
<table class="table table-striped">
	<tr style="background-color: lightgrey;">
		<th>STATEID</th>
		<th>STATENAME</th>
		<th>CREATIONDATE</th>
		<th>UPDATIONDATE</th>
		<th>Edit</th>
		<th>Delete</th>
	</tr>
	<% for(var i=0; i < data.length; i++) {%>
	<tr>
		<td><%= data[i].STATEID %> </td>
		<td><%= data[i].STATENAME %> </td>
		<td><%= data[i].CREATIONDATE %> </td>
		<td><%= data[i].UPDATIONDATE %> </td>
		<td><a href="/admin/state/update?stateid=<%= data[i].STATEID %>" data-toggle="tooltip" title="edit" id="edit"><span class="glyphicon glyphicon-edit">Edit</span></a></td>
		<td><a href="/admin/state/delete?stateid=<%= data[i].STATEID %>" data-toggle="tooltip" title="delete"><span class="glyphicon glyphicon-trash"></span></a></td>
		
	</tr>
	<% } %>
</table>
</div>
<script>
	$(document).ready(function(){
		$('[data-toggle="tooltip"]').tooltip();
		$('[data-toggle="tooltip"]').tooltip();
	});
</script>
<script src="/js/jquery.js"></script>
<script src="/js/bootstrap.js"></script>
</body>
</html>