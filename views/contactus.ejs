<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<title>Free Ad Posting | Free Web Directory</title>
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link rel="stylesheet" href="/css/bootstrap.css">
	<link rel="stylesheet" href="/css/style.css">
	<link rel="stylesheet" href="/css/home1.css">
	<link rel="stylesheet" href="/css/contactus.css">
	<link rel="icon" href="/css/favicon.ico">
	<script src="https://use.fontawesome.com/5ee69e6b7c.js"></script>
</head>
<body>
<%- include homecomman.ejs %>
<%- include homecommanfooter.ejs %>	
<!-- middle section -->

<div class="header">
	<div class="container-fluid" id="middle">
		<div class="row">
			<div id="slide1" class="col-lg-3 col-sm-3">
				
				 <div class="wrapper">
            <!-- Sidebar Holder -->
            <nav id="sidebar">
                
0
                <ul class="list-unstyled components" >
                    	<% for(var i=0; i< categorydata[0].length; i++) {%>
					  <li class="dropdown">
					    <a class="dropdown-toggle" data-toggle="dropdown" href="#"><%= categorydata[0][i].CATEGORYNAME%>
					    <span class="caret"></span></a>
					    <ul class="dropdown-menu">
					    	<% 
							var categoryid  = categorydata[0][i].CATEGORYID;
							if (categorydata[categoryid]) {
							var newArr = categorydata[categoryid];
							for(var j=0; j< newArr.length; j++) {
							%>
					      <li><a href="/listing"><%= newArr[j].CATEGORYNAME%></a></li>
							<% } 
							}
							%>	
					    </ul>
					 	</li>
					  <% } %>
                </ul>
            </nav>
</div>
			</div>
			<div id="main" class="col-lg-9 col-sm-9">
			<div class="span4">
            <section id="contact">
            <div class="section-content">
                <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d1884.5787292691523!2d73.00569542578258!3d19.1445836467463!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x3be7bf5847d10205%3A0xdb3ea4f510398c79!2sGautam+Nagar%2C+MIDC+Industrial+Area%2C+Rabale%2C+Navi+Mumbai%2C+Maharashtra+400701!5e0!3m2!1sen!2sin!4v1509866082773" width="100%" height="200" frameborder="0" style="border:0" allowfullscreen></iframe>
                <h1 class="blink"><span id="icon" style="color:orange;" class="glyphicon glyphicon-phone"></span> CONTACT WITH US <span id="icon" style="color:orange;" class="glyphicon glyphicon-phone "></span></h1>
            </div>
            <div class="contact-section">
            <div class="container-fluid" id="cont" style="margin-bottom: 72px;">
                <form action="/addcontactusdata" method="post" name="formname" data-toggle="validator" onsubmit="return validation();">
                    <div class="col-md-5 form-line">
                        <div class="form-group">
                            <label for="exampleInputUsername">Your name</label>
                            <input type="text" class="form-control" id="username" name="username" placeholder=" Enter Name">
                        </div>
                        <p id="error" style="color: red;"></p>
                        <div class="form-group">
                            <label for="exampleInputEmail">Email Address</label>
                            <input type="text" class="form-control" name="emailid" id="emailid" placeholder=" Enter Email id">
                        </div>
                        <p id="error1" style="color: red;"></p>  
                        <div class="form-group">
                            <label for="telephone">Mobile No.</label>
                            <input type="number" class="form-control" name="mobileno" id="mobileno" placeholder=" Enter 10-digit mobile no.">
                        </div>
                        <p id="error2" style="color: red;"></p>
                    </div>
                    <div class="col-md-7">
                        <div class="form-group">
                            <label for ="description"> Message</label>
                            <textarea rows="1" class="form-control" name="message" id="message" placeholder="Enter Your Message"></textarea>
                        </div>
                        <p id="error3" style="color: red;"></p>
                        <div>
                            <button type="submit" class="btn btn-default submit"><i class="fa fa-paper-plane" aria-hidden="true"></i>  Send Message</button>
                        </div>
                        
                    </div>
                </form>
            </div>
        	</section>
		</div>
	</div>
<!-- end of middle section -->

		</div>
	</div>
</div>
	<script src="/js/contactusvalidation.js"></script>
	
</body>
</html>